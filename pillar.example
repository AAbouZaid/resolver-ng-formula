resolver:
  use_resolvconf: True # used to uninstall resolvconf and salt manage the config
  ng:
    resolvconf:
      enabled: False
    networkmanager:
      file: /etc/NetworkManager/NetworkManager.conf
  domain: example.com
  nameservers:
    - 8.8.8.8
    - 8.8.4.4
  searchpaths:
    - example.com
  options:
    - rotate
    - timeout:1
    - attempts:5
